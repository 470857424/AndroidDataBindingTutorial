<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.github.captain_miao.databinding.tutorial"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.github.captain_miao.databinding.tutorial.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.github.captain_miao.databinding.tutorial.SimpleActivityA"
                  android:label="@string/Activity_A">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:scheme="tutorial" android:host="databinding" android:pathPrefix="/activity_a" />
            </intent-filter>
        </activity>
        <activity android:name="com.github.captain_miao.databinding.tutorial.SimpleActivityB"
                  android:label="@string/Activity_B">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:scheme="tutorial" android:host="databinding" android:pathPrefix="/activity_b" />
            </intent-filter>
        </activity>
        <activity android:name="com.github.captain_miao.databinding.tutorial.SimpleActivityC"
            android:label="@string/Activity_C">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:scheme="tutorial" android:host="databinding" android:pathPrefix="/activity_c" />
            </intent-filter>
        </activity>
        <activity android:name="com.github.captain_miao.databinding.tutorial.recycleview.RecycleViewActivity"
            android:label="@string/Activity_D">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:scheme="tutorial" android:host="databinding" android:pathPrefix="/activity_d" />
            </intent-filter>
        </activity>



    </application>

</manifest>